$("#main").html("<%=j render partial: "page" %>");

<% @users.each do |user| %>
  $("#main").append('<div class="row"> <%=j render partial: "user", object: user %></div><br />');
<% end %>

var paginator = new Paginator($(".pagination ul"), <%= params[:page].to_i %>, <%= @pages %>);
paginator.addPagination();
bindClickEvents($(".container"), "<%= request.path.strip %>", "<%= params[:user] %>");
